{
  "manifest_version": 2,
  "name": "Wallpaper Buddy",
  "description": "A Simple Extension Which Makes your image search easy to download",
  "version": "1.0",
  "author": "Amit Gujar",
  "browser_action": {
    "default_icon": "/icon.jpg",
    "default_title": "Enter Your Image"
  },
  "web_accessible_resources": ["scripts/*", "static/images/*"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["scripts/main.js"]

  }],
  "permissions": ["activeTab"]
}